<?xml version="1.0"?>
<robot name="bot">
  <link name="base_link">
    <visual>
      <geometry>

        <box size="0.3 0.15 0.1" />
      </geometry>
      <origin xyz="0 0 0.07"/>
    </visual>
  </link>
 <link name="wheel_right">
	<visual>
		<geometry>
			<cylinder length="0.04"  radius="0.06" />.
				</geometry>
	<origin  rpy="1.57075 1.57075 0" xyz="0.15 0.10 0.02" />
	<material name="white"/>
	</visual>
</link> 
<link name="wheel_left">
	<visual>
		<geometry>
			<cylinder length="0.04"  radius="0.06" />
				</geometry>
	<origin  rpy="1.57075 1.57075 0" xyz="0.15 -0.10 0.02" />
	<material name="white"/>
	</visual>
</link>
<joint name="base_to_left_leg" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_left"/>
    <origin xvz="0.15 -0.10 0.02"/>
</joint> 
<link name="wheel_rb">
 <visual>
   <geometry>
	<cylinder length="0.04"  radius="0.06" />
				</geometry>
	<origin  rpy="1.57075 1.57075 0" xyz="-0.15 0.10 0.02" />
	<material name="white"/>
	</visual>
</link> 
<link name="wheel_lb">
 <visual>
   <geometry>
	<cylinder length="0.04"  radius="0.06" />
				</geometry>
	<origin  rpy="1.57075 1.57075 0" xyz="-0.15 -0.1 0.02" />
	<material name="White"/>
	</visual>
</link> 

<joint name="base_to_left_legback" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_lb"/>
    <origin xvz="-0.15 -0.1 0.02"/>
</joint>


<joint name="base_to_right_leg" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right"/>
    <origin xvz="0.15 0.10 0.02"/>
</joint>
	
<joint name="base_to_right_legback" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_rb"/>
    <origin xvz="-0.15 0.1 0.02"/>
</joint>
	
</robot>
