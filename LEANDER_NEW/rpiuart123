import serial
import base64


with open(r'/home/leander/Desktop/yosemite.png', "rb") as imageFile:
    str = base64.b64encode(imageFile.read())
    print(str)



ser = serial.Serial(
    port='/dev/serial0',
    baudrate=115200,
    parity=serial.PARITY_NONE,
    stopbits=serial.STOPBITS_ONE,
    bytesize=serial.EIGHTBITS,
    timeout=1
)

ser.write(str)





