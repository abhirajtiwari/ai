    <parent link="base_link"/>
    <child link="base_camera_front"/>
</joint>

<!--..................................SENSOR PLUGIN.........................................-->
<!-- camera -->
  <gazebo reference="base_camera_front">
    <sensor type="camera" name="camera1">
      <update_rate>30.0</update_rate>
      <camera name="head">
        <horizontal_fov>1.3962634</horizontal_fov>
        <image>
          <width>800</width>
          <height>800</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>0.02</near>
          <far>300</far>
        </clip>
        <noise>
          <type>gaussian</type>
          <!-- Noise is sampled independently per pixel on each frame.
               That pixel's noise value is added to each of its color
               channels, which at that point lie in the range [0,1]. -->
          <mean>0.0</mean>
          <stddev>0.007</stddev>
        </noise>
      </camera>
      <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
        <alwaysOn>true</alwaysOn>
        <updateRate>0.0</updateRate>
        <cameraName>rrbot/camera1</cameraName>
        <imageTopicName>image_raw</imageTopicName>
        <cameraInfoTopicName>camera_info</cameraInfoTopicName>
        <frameName>camera_link</frameName>
        <hackBaseline>0.07</hackBaseline>
        <distortionK1>0.0</distortionK1>
        <distortionK2>0.0</distortionK2>
        <distortionK3>0.0</distortionK3>
        <distortionT1>0.0</distortionT1>
        <distortionT2>0.0</distortionT2>
      </plugin>
    </sensor>
  </gazebo>


<!--.................................2D LIDAR.......................................-->

<link name="base_lidar_front">

        <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>

    <collision name='collision'>

      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
	<origin xyz="0 0 0" rpy="0 0 0"/>
   	<material name="red"/>
    </collision>

    <visual>

      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
	<origin xyz="0 0 0" rpy="0 0 0"/>
	<material name="red"/>

    </visual>
</link>

<gazebo reference="base_lidar_front">
    <material>Gazebo/Red</material>
    <turnGravityOff>false</turnGravityOff>
</gazebo>

<joint name="lidar_front_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin rpy="0 0 0" xyz="0 0 0.155" />
    <parent link="base_link"/>
    <child link="base_lidar_front"/>
</joint>

<!--.................................SENSOR PLUGIN.....................................-->

  <gazebo reference="base_lidar_front">
    <sensor type="ray" name="lidar">
      <pose>0 0 0 0 0 0</pose>
      <visualize>false</visualize>
      <update_rate>50</update_rate>
      <ray>
         <scan>
            <horizontal>
               <samples>11</samples>    <!--180/10-->
               <resolution>1</resolution>
               <min_angle>-1.570795</min_angle>
               <max_angle>1.570795</max_angle>
            </horizontal>
            <vertical>
               <samples>1</samples>
               <resolution>1</resolution> 
          <max_angle>0.14835</max_angle>
          <min_angle>0</min_angle>
            </vertical>
         </scan>
         <range>
            <min>0.1</min>
            <max>3.0</max>
            <resolution>0.01</resolution>
         </range>
      </ray>
      <plugin filename="libgazebo_ros_laser.so" name="gazebo_lidar">
         <gaussianNoise>0.005</gaussianNoise>
         <alwaysOn>true</alwaysOn>
         <update_rate>50</update_rate>
         <updateRateHZ>10.0</updateRateHZ>
         <topicName>lidar</topicName>
         <frameName>base_lidar_front</frameName>
      </plugin>
   </sensor>
  </gazebo>





<!--.................................DIFFERENTIAL DRIVES..........................................-->

<gazebo>
  <plugin name="differential_drive_controller1" filename="libgazebo_ros_diff_drive.so">
    <alwaysOn>true</alwaysOn>
    <updateRate>50</updateRate>
    <leftJoint>base_to_left_leg</leftJoint>
    <rightJoint>base_to_right_leg</rightJoint>
    <wheelSeparation>0.6</wheelSeparation>
    <wheelDiameter>0.2</wheelDiameter>
