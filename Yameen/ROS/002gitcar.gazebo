<?xml version="1.0"?>
<robot name="nice_ride">

 <!--BASE-->
    <link name="base_link">

<inertial>
        <mass value="0.18454"/>
        <inertia ixx="0.0159021" ixy="0" ixz="0" iyy="0.0159021" iyz="0" izz="0.0307567"/>
</inertial>

 <collision name='collision'>
         <geometry>
          <box size="0.8 0.6 0.1"/>
             <origin xyz="0 0 0.1"/>
        </geometry>
 </collision>

  <visual>

      <geometry>
        <box size="0.8 0.6 0.1"/>
      </geometry>
  <origin xyz="0 0 0.1"/>
  </visual>

  </link>
    <gazebo reference="base_link">
    <material>Gazebo/Blue</material>
    <turnGravityOff>false</turnGravityOff>
  </gazebo>

<!--RIGHT WHEEL-->

 <link name="right_wheel">

     <inertial>
          <mass value="1"/>
        <inertia ixx="0.145833" ixy="0" ixz="0" iyy="0.145833" iyz="0" izz="0.125"/>
</inertial>

<collision name='collision'>
         <geometry>
         <cylinder length="0.1" radius="0.1"/>
        </geometry>
        <origin rpy="-1.570795 -1.570795 0" xyz="0 0 0"/>
   <material name="black"/>
 </collision>


    <visual>
      <geometry>
         <cylinder length="0.1" radius="0.1"/>
      </geometry>
      <origin rpy="-1.570795 -1.570795 0" xyz="0 0 0"/>
   <material name="black"/>
    </visual>
  </link>
    <gazebo reference="right_wheel">
    <material>Gazebo/Black</material>
    <turnGravityOff>false</turnGravityOff>
  </gazebo>

  <joint name="joint_right_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="right_wheel"/>
    <origin xyz="-0.3 0.35 0"/>
      <axis xyz="0 1 0"/>

      <calibration rising="0.0"/>
      <dynamics damping="0.0" friction="0.0"/>

  </joint>


    <!--LEFT WHEEL-->
<link name="left_wheel">

    <inertial>
        <mass value="1"/>
        <inertia ixx="0.145833" ixy="0" ixz="0" iyy="0.145833" iyz="0" izz="0.125"/>
</inertial>

<collision name='collision'>
         <geometry>
         <cylinder length="0.1" radius="0.1"/>
        </geometry>
      <origin rpy="-1.570795 -1.570795 0" xyz="0 0 0"/>
   <material name="black"/>
 </collision>

    <visual>
      <geometry>
         <cylinder length="0.1" radius="0.1"/>
      </geometry>
      <origin rpy="-1.570795 -1.570795 0" xyz="0 0 0"/>
   <material name="black"/>
    </visual>
  </link>
    <gazebo reference="left_wheel">
    <material>Gazebo/Black</material>
    <turnGravityOff>false</turnGravityOff>
  </gazebo>

  <joint name="joint_left_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel"/>
    <origin xyz="-0.3 -0.35 0"/>
      <axis xyz="0 1 0"/>

      <calibration rising="0.0"/>
      <dynamics damping="0.0" friction="0.0"/>


  </joint>

    <!--LEFT FRONT WHEEL-->
<link name="left_front_wheel">

    <inertial>
        <mass value="1"/>
        <inertia ixx="0.145833" ixy="0" ixz="0" iyy="0.145833" iyz="0" izz="0.125"/>
</inertial>

<collision name='collision'>
         <geometry>
         <cylinder length="0.1" radius="0.1"/>
        </geometry>
     <origin rpy="-1.570795 -1.570795 0" xyz="0 0 0"/>
   <material name="black"/>
 </collision>

    <visual>
      <geometry>
         <cylinder length="0.1" radius="0.1"/>
      </geometry>
      <origin rpy="-1.570795 -1.570795 0" xyz="0 0 0"/>
   <material name="black"/>
    </visual>
  </link>
    <gazebo reference="left_front_wheel">
    <material>Gazebo/Black</material>
    <turnGravityOff>false</turnGravityOff>
  </gazebo>

  <joint name="joint_left_front_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="left_front_wheel"/>
    <origin xyz="0.3 -0.35 0"/>
      <axis xyz="0 1 0"/>

      <calibration rising="0.0"/>
      <dynamics damping="0.0" friction="0.0"/>

  </joint>

     <!--RIGHT FRONT WHEEL-->
<link name="right_front_wheel">

    <inertial>
        <mass value="1"/>
        <inertia ixx="0.145833" ixy="0" ixz="0" iyy="0.145833" iyz="0" izz="0.125"/>
</inertial>

<collision name='collision'>
         <geometry>
         <cylinder length="0.1" radius="0.1"/>
        </geometry>
      <origin rpy="-1.570795 -1.570795 0" xyz="0 0 0"/>
   <material name="black"/>
 </collision>
    <visual>

      <geometry>
         <cylinder length="0.1" radius="0.1"/>
      </geometry>
      <origin rpy="-1.570795 -1.570795 0" xyz="0 0 0"/>
   <material name="black"/>
    </visual>
  </link>

    <gazebo reference="right_front_wheel">
    <material>Gazebo/Black</material>
    <turnGravityOff>false</turnGravityOff>
  </gazebo>

  <joint name="base_to_right_front_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="right_front_wheel"/>
    <origin xyz="0.3 0.35 0"/>
      <axis xyz="0 1 0"/>

      <calibration rising="0.0"/>
      <dynamics damping="0.0" friction="0.0"/>

  </joint>




<!--................................................................................................-->



<!--..............................................IMU SENSOR...........................................-->
<link name="imu">

        <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>

    <collision name='collision'>

      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
  <origin xyz="0 0 0" rpy="0 0 0"/>
    <material name="black"/>
    </collision>

    <visual>

      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
  <origin xyz="0 0 0" rpy="0 0 0"/>
  <material name="black"/>

    </visual>
</link>

<gazebo reference="imu">
    <material>Gazebo/Black</material>
    <turnGravityOff>false</turnGravityOff>
</gazebo>

<joint name="imu_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin rpy="0 0 0" xyz="0 0 0.155" />
    <parent link="base_link"/>
    <child link="imu"/>
</joint>

<!--....................................PLUGIN.........................................-->
  <gazebo reference="imu">
    <gravity>true</gravity>
    <sensor name="imu_sensor" type="imu">
      <always_on>true</always_on>
      <update_rate>100</update_rate>
      <visualize>true</visualize>
      <topic>__default_topic__</topic>
      <plugin filename="libgazebo_ros_imu_sensor.so" name="imu_plugin">
        <topicName>imu</topicName>
        <bodyName>imu_link</bodyName>
        <updateRateHZ>10.0</updateRateHZ>
        <gaussianNoise>0.0</gaussianNoise>
        <xyzOffset>0 0 0</xyzOffset>
        <rpyOffset>0 0 0</rpyOffset>
        <frameName>imu_link</frameName>
      </plugin>
      <pose>0 0 0 0 0 0</pose>
    </sensor>
  </gazebo>


<!--..................................................................................-->


<!--...................................GPS MODULE...................................................-->
<link name="GPS">

        <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>

    <collision name='collision'>

      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
  <origin xyz="0 0 0" rpy="0 0 0"/>
    <material name="green"/>
    </collision>

    <visual>

      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
  <origin xyz="0 0 0" rpy="0 0 0"/>
  <material name="green"/>

    </visual>
</link>

<gazebo reference="GPS">
    <material>Gazebo/Green</material>
    <turnGravityOff>false</turnGravityOff>
</gazebo>

<joint name="gps_jpint" type="fixed">
    <axis xyz="0 1 0" />
    <origin rpy="0 0 0" xyz="-0.395 -0.295 0.155" />
    <parent link="base_link"/>
    <child link="GPS"/>
</joint>

<!--..................................GPS SENSOR PLUGIN.......................................-->

    <!-- GPS Sensor -->
  <gazebo>
    <plugin name="novatel_gps_sim" filename="libhector_gazebo_ros_gps.so">
      <alwaysOn>1</alwaysOn>
      <updateRate>50</updateRate>
      <bodyName>GPS</bodyName>
      <topicName>gps</topicName>
      <velocityTopicName>gps_velocity</velocityTopicName>
      <drift>5.0 5.0 5.0</drift>
      <gaussianNoise>0.1 0.1 0.1</gaussianNoise>
      <velocityDrift>0 0 0</velocityDrift>
      <velocityGaussianNoise>0.1 0.1 0.1</velocityGaussianNoise>
      <heading_deg>90</heading_deg> 
    </plugin>
  </gazebo>


<link name="lidar_sensor">

        <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>

    <collision name='collision'>

      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
  <origin xyz="0 0 0" rpy="0 0 0"/>
    <material name="white"/>
    </collision>

    <visual>

      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
  <origin xyz="0 0 0" rpy="0 0 0"/>
  <material name="white"/>

    </visual>
</link>

<gazebo reference="lidar_sensor">
    <material>Gazebo/White</material>
    <turnGravityOff>false</turnGravityOff>
</gazebo>

<joint name="lidar_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin rpy="0 0 0" xyz="0.395 0 0.155" />
    <parent link="base_link"/>
    <child link="lidar_sensor"/>
</joint>

<!--..................................SENSOR PLUGIN.........................................-->
<!-- camera -->
  <gazebo reference="lidar_sensor">
    <sensor type="ray" name="lidar">
      <pose>0 0 0 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>50</update_rate>
      <ray>
         <scan>
            <horizontal>
               <samples>10</samples>
               <resolution>1</resolution>
               <min_angle>-1.570795</min_angle>
               <max_angle>1.570795</max_angle>
            </horizontal>
            <vertical>
               <samples>1</samples>
               <resolution>1</resolution> 
          <max_angle>0.14835</max_angle>
          <min_angle>0</min_angle>
            </vertical>
         </scan>
         <range>
            <min>0.1</min>
            <max>30.0</max>
            <resolution>0.01</resolution>
         </range>
      </ray>
      <plugin filename="libgazebo_ros_laser.so" name="gazebo_lidar">
         <gaussianNoise>0.005</gaussianNoise>
         <alwaysOn>true</alwaysOn>
         <update_rate>50</update_rate>
         <updateRateHZ>10.0</updateRateHZ>
         <topicName>sensor/lidar</topicName>
         <frameName>lidar_sensor</frameName>
      </plugin>
   </sensor>
  </gazebo>

<!--................................................................................................-->

<gazebo>
  <plugin name="differential_drive_controller1" filename="libgazebo_ros_diff_drive.so">
    <alwaysOn>true</alwaysOn>
    <updateRate>50</updateRate>
    <leftJoint>joint_left_wheel</leftJoint>
    <rightJoint>joint_right_wheel</rightJoint>
    <wheelSeparation>0.6</wheelSeparation>
    <wheelDiameter>0.2</wheelDiameter>
    <torque>30</torque>
    <commandTopic>velocity</commandTopic>
    <odometryTopic>odom</odometryTopic>
    <odometryFrame>odom</odometryFrame>
    <robotBaseFrame>base_link</robotBaseFrame>
    <robotNamespace>ddrobot</robotNamespace>
  </plugin>
</gazebo>

    <gazebo>
  <plugin name="differential_drive_controller2" filename="libgazebo_ros_diff_drive.so">
    <alwaysOn>true</alwaysOn>
    <updateRate>50</updateRate>
    <leftJoint>joint_left_front_wheel</leftJoint>
    <rightJoint>base_to_right_front_wheel</rightJoint>
    <wheelSeparation>0.6</wheelSeparation>
    <wheelDiameter>0.2</wheelDiameter>
    <torque>30</torque>
    <commandTopic>velocity</commandTopic>
    <odometryTopic>odom</odometryTopic>
    <odometryFrame>odom</odometryFrame>
    <robotBaseFrame>base_link</robotBaseFrame>
    <robotNamespace>ddrobot</robotNamespace>
  </plugin>
</gazebo>



</robot>